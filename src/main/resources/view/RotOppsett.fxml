<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane styleClass="pane" prefHeight="400.0" prefWidth="1100.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ae.controller.RotOppsettController">
    <top>
        <MenuBar styleClass="menuBar" BorderPane.alignment="CENTER">
            <menus>
                <Menu styleClass="menu" mnemonicParsing="false" text="Meny">
                    <items>
                  <MenuItem styleClass="menuItem" mnemonicParsing="false" onAction="#gåTilKundeoversikt" text="Kunder" />
                        <SeparatorMenuItem mnemonicParsing="false" />
                  <MenuItem styleClass="menuItem" mnemonicParsing="false" onAction="#gåTilSkademeldingoversikt" text="Skademelding" />
                        <SeparatorMenuItem mnemonicParsing="false" />
                  <MenuItem styleClass="menuItem" mnemonicParsing="false" onAction="#gåTilForsikringoversikt" text="Forsikring" />
                        <SeparatorMenuItem mnemonicParsing="false" />
                        <MenuItem styleClass="menuItem" mnemonicParsing="false" onAction="#avsluttTrykket" text="Avslutt" />
                    </items>
                </Menu>
                <Menu styleClass="menu" fx:id="filMenu" mnemonicParsing="false" text="Fil">
                    <items>
                        <MenuItem styleClass="menuItem" mnemonicParsing="false" onAction="#lagreFilTrykket" text="Lagre til fil" />
                        <SeparatorMenuItem mnemonicParsing="false" />
                        <MenuItem styleClass="menuItem" fx:id="hentFilMenuItem" mnemonicParsing="false" onAction="#hentFilTrykket" text="Hent fra til" />
                    </items>
                </Menu>
                <Menu styleClass="menu" mnemonicParsing="false" text="Hjelp">
                    <items>
                        <MenuItem styleClass="menuItem" mnemonicParsing="false" onAction="#visOmOss" text="Om oss" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
    </top>
    <center>
        <Label text="Velg handlinger under Meny i menylinjen" BorderPane.alignment="CENTER" />
    </center>
</BorderPane>
